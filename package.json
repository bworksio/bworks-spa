{
  "name": "b-works",
  "version": "2.0.0",
  "description": "B-Works SPA based on full-featured Webpack + vue-loader setup with hot reload, linting & css extraction.",
  "author": "Stefan Kudwien <stefan.kudwien@b-works.io>",
  "private": true,
  "scripts": {
    "dev": "DEBUG=axios node server",
    "start": "NODE_ENV=production node server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "now-start": "node server",
    "now-build": "",
    "lint": "eslint --ext .js,.vue src"
  },
  "engines": {
    "node": ">=7.0",
    "npm": ">=4.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "axios-debug-log": "^0.3.0",
    "babel-runtime": "^6.26.0",
    "compression": "^1.7.0",
    "cross-env": "^5.0.1",
    "es6-promise": "^4.1.1",
    "expose-loader": "^0.7.3",
    "express": "^4.15.3",
    "flexslider": "^2.6.3",
    "fullpage.js": "^2.9.4",
    "image-preloader": "^1.1.1",
    "jquery": "^3.2.1",
    "jump.js": "^1.0.2",
    "locutus": "^2.0.8",
    "lru-cache": "^4.1.1",
    "pm2": "^2.6.1",
    "raven-js": "^3.17.0",
    "scrollmagic": "^2.0.5",
    "serve-favicon": "^2.4.3",
    "sitemap": "^1.12.0",
    "vue": "^2.4.1",
    "vue-analytics": "^4.1.2",
    "vue-i18n": "^7.0.5",
    "vue-router": "^2.7.0",
    "vue-server-renderer": "^2.4.1",
    "vue2-google-maps": "^0.7.5",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^2.0.0",
